{
    "grammar": {
        "language": "mysql",
        "version": {
            "min": "5.7",
            "max": "5.7.9999"
        }
    },
    "rules": {
        "comment": {
            "starter": true,
            "case_sensitive": false,
            "group": [
                {
                    "tokenizer": {
                        "start": "/*",
                        "end": "*/"
                    }
                },
                {
                    "tokenizer": {
                        "start": "\\-\\-\\s",
                        "end": "\n"
                    }
                }
            ]
        },
        "create_table": {
            "starter": true,
            "case_sesnsitive": false,
            "keywords": [
                "CREATE"
            ]
        }
    }
}
